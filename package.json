{
    "name": "ncorp.launcher.web",
    "private": true,
    "version": "1.0.2",
    "type": "module",
    "scripts": {
        "dev": "cp .env.development .env && dotenv -e .env.development -- cross-env NODE_ENV=development vite dev",
        "shade:add": "bun x shadcn-svelte@next add",
        "build": "cp .env.production .env && dotenv -e .env.production -- vite build",
        "start": "cp .env.production .env && dotenv -e .env.production -- bun run index.js",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "db:full": "bunx @better-auth/cli migrate --config ./src/lib/auth/server.ts && bunx @better-auth/cli generate --config ./src/lib/auth/server.ts",
        "prod": "bun run build && npm run deploy",
        "genmodels": "cross-env JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" openapi-generator-cli generate -i https://agent.n-lan.com:3213/swagger/v1/swagger.json -o src/lib/shared-models -g typescript-fetch --additional-properties=SupportsES6=true,generateApis=false",
        "genserver": "cross-env JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" openapi-generator-cli generate -i https://agent.n-lan.com:3213/swagger/v1/swagger.json -o src/lib/api/server -g typescript-fetch --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,modelPackage=../../common/models,apiPackage=apis,apiPackagePrefix=server",
        "genagent": "cross-env JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" openapi-generator-cli generate -i https://agent.n-lan.com:3214/swagger/v1/swagger.json -o src/lib/api/agent -g typescript-fetch --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,modelPackage=../../common/models,apiPackage=apis,apiPackagePrefix=agent",
        "genall": "bun genmodels && bun genserver && bun genagent"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.9",
        "@eslint/js": "^9.27.0",
        "@iconify-json/svg-spinners": "^1.2.2",
        "@sveltejs/adapter-node": "^5.2.12",
        "@sveltejs/kit": "^2.21.1",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "@types/node": "^22.15.21",
        "@vite-pwa/assets-generator": "^1.0.0",
        "autoprefixer": "^10.4.21",
        "bits-ui": "^1.8.0",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^8.0.0",
        "embla-carousel-svelte": "^8.6.0",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-svelte": "^3.9.0",
        "formsnap": "^2.0.1",
        "globals": "^16.1.0",
        "polka": "^0.5.2",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.33.1",
        "svelte-check": "^4.2.1",
        "svelte-sonner": "^1.0.3",
        "sveltekit-adapter-iis": "^1.4.1",
        "tailwind-merge": "^3.3.0",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.1",
        "vite": "^6.3.5"
    },
    "dependencies": {
        "@cartamd/plugin-emoji": "^4.3.0",
        "@dicebear/collection": "^9.2.2",
        "@dicebear/core": "^9.2.2",
        "@fontsource-variable/inter": "^5.2.5",
        "@fontsource-variable/rubik": "^5.2.5",
        "@iconify/tailwind": "^1.2.0",
        "@lucide/svelte": "^0.511.0",
        "@microsoft/signalr": "^8.0.7",
        "@prisma/client": "^6.8.2",
        "@pyncz/tailwind-mask-image": "^2.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "better-auth": "^1.2.8",
        "bigger-picture": "^1.1.19",
        "carta-md": "^4.10.2",
        "datatables.net": "^2.3.1",
        "datatables.net-buttons-dt": "^3.2.3",
        "datatables.net-colreorder-dt": "^2.1.0",
        "datatables.net-dt": "^2.3.1",
        "datatables.net-fixedcolumns-dt": "^5.0.4",
        "datatables.net-fixedheader-dt": "^4.0.2",
        "datatables.net-plugins": "^2.3.0",
        "datatables.net-responsive-dt": "^3.0.4",
        "datatables.net-scroller-dt": "^2.4.3",
        "datatables.net-select-dt": "^3.0.0",
        "dayjs": "^1.11.13",
        "google-translate-api-x": "^10.7.2",
        "isomorphic-dompurify": "^2.25.0",
        "mediasoup-client": "^3.11.0",
        "mini-svg-data-uri": "^1.4.4",
        "moment": "^2.30.1",
        "mysql2": "^3.14.1",
        "pino": "^9.7.0",
        "pino-pretty": "^13.0.0",
        "prisma": "6.7.0",
        "socket.io-client": "^4.8.1",
        "superjson": "^2.2.2",
        "svelte-inview": "^4.0.4",
        "svelte-motion": "^0.12.2",
        "svelte-persisted-store": "^0.12.0",
        "sveltekit-i18n": "^2.4.2",
        "sveltekit-superforms": "^2.25.0",
        "tldts": "^7.0.7",
        "zod": "^3.25.28"
    }
}
